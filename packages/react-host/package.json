{
  "name": "@appshell/react-host",
  "version": "0.2.2",
  "description": "Appshell host for React",
  "main": "index.js",
  "private": true,
  "scripts": {
    "generate:env": "appshell generate env -e .env --prefix $APPSHELL_ENV_PREFIX",
    "generate:index": "appshell generate index --validate-registry-ssl-cert $APPSHELL_VALIDATE_REGISTRY_SSL_CERT --registry $APPSHELL_ADJUNCT_REGISTRY $APPSHELL_REGISTRY",
    "generate:metadata": "appshell generate metadata --validate-registry-ssl-certinsecure $APPSHELL_VALIDATE_REGISTRY_SSL_CERT --registry $APPSHELL_ADJUNCT_REGISTRY $APPSHELL_REGISTRY",
    "start": "dotenv -- run-p start:appshell start:run",
    "start:appshell": "appshell start --host --validate-registry-ssl-cert $APPSHELL_VALIDATE_REGISTRY_SSL_CERT --registry $APPSHELL_REGISTRY --adjunct-registry $APPSHELL_ADJUNCT_REGISTRY --env $APPSHELL_ENV --env-prefix $APPSHELL_ENV_PREFIX --env-global-name $APPSHELL_ENV_GLOBAL_VAR",
    "start:run": "webpack-cli serve --mode development",
    "prebuild": "yarn clean",
    "build": "webpack --mode production",
    "build:development": "webpack --mode development",
    "serve": "dotenv -- run-s generate:* serve:run",
    "serve:run": "serve dist -p $APPSHELL_PORT",
    "serve:developer": "dotenv -- run-p start:appshell serve:run",
    "clean": "rm -rf dist"
  },
  "author": "Robert Hamilton <rh@navaris.com>",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [
    "webpack",
    "module federation",
    "micro-frontends",
    "appshell"
  ],
  "license": "MIT"
}