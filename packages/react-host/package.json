{
  "name": "@appshell/react-host",
  "version": "0.1.0",
  "description": "Appshell host for React",
  "main": "index.js",
  "private": true,
  "scripts": {
    "generate:env": "appshell generate env -e .env --prefix $APPSHELL_ENV_PREFIX --outDir dist",
    "generate:manifest": "appshell generate manifest --configsDir $APPSHELL_CONFIGS_DIR --outDir dist --depth $APPSHELL_CONFIGS_DEPTH && yarn merge:manifest",
    "merge:manifest": "appshell merge manifest --outDir dist --manifest $APPSHELL_BASE_MANIFESTS dist/appshell.manifest.json",
    "watch:configs": "nodemon --watch $APPSHELL_CONFIGS_DIR --ext json --exec yarn generate:manifest",
    "start": "dotenv -- run-p generate:env watch:configs start:run",
    "start:run": "webpack-cli serve --mode development",
    "prebuild": "yarn clean",
    "build": "webpack --mode production",
    "build:development": "webpack --mode development",
    "serve": "dotenv -- run-s generate:* serve:run",
    "serve:run": "serve dist -p $APPSHELL_PORT",
    "serve:developer": "dotenv -- run-s generate:* build:development serve:run",
    "clean": "rm -rf dist"
  },
  "author": "Robert Hamilton <rh@navaris.com>",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [
    "webpack",
    "module federation",
    "micro-frontends",
    "appshell"
  ],
  "license": "MIT"
}
